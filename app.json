{
  "expo": {
    "name": "Chytrý Skleník",
    "slug": "chytrysklenik",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "me.ganamaga.smartstuff.sklenik"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "me.ganamaga.smartstuff.sklenik"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "entryPoint": "app/entry.js",
    "extra": {
      "router": {},
      "eas": {
        "projectId": "c44e6dc7-2fad-4b9e-ba60-70e534a809bd"
      }
    },
    "owner": "ganamaga"
  }
}
